<h1>New Project</h1>

<%= form_for @project do |f| %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div></br>

  <div class="field">
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div></br>

  <div class="field">
    <%= f.label :goal %>
    <%= f.number_field :goal %>
  </div></br>

  <div class="field">
    <%= f.label :end_date %>
    <%= f.date_field :end_date %>
  </div></br>

  <div class="field">
    <%= f.submit "Submit" %>
  </div></br>
<% end %>
